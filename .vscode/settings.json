{
    "git.ignoreLimitWarning": true,
    "go.useLanguageServer": true,
    "go.formatTool": "gofmt",
    "[go]": {
        "editor.formatOnSave": false,
        "editor.formatOnType": false
    },
    "go.lintTool": "gometalinter",
    "go.lintFlags": [
        "--exclude=\"\bexported \\w+ (\\S*['.]*)([a-zA-Z'.*]*) should have comment or be unexported\b\""
    ],
}
